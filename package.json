{
  "name": "ruby-china-graphql-api",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "dev:front": "parcel playground/index.html",
    "dev:schema": "graphql-codegen -w --config codegen.yml",
    "build": "yarn build:schema && yarn build:front && yarn build:server",
    "build:server": "tsc",
    "build:front": "parcel build playground/index.html --out-dir public",
    "build:schema": "graphql-codegen --config codegen.yml",
    "start": "node dist/index.js",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "apollo-datasource-rest": "^0.9.3",
    "apollo-server-fastify": "^2.17.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.3.0",
    "fastify-oauth2": "^4.1.0",
    "fastify-static": "^3.2.0",
    "graphiql": "^1.0.3",
    "graphiql-explorer": "^0.6.2",
    "graphql": "^15.3.0",
    "graphql-tools": "^6.2.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.20",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-use": "^15.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-resolvers": "^1.17.9",
    "@types/lodash": "^4.14.161",
    "@types/node": "^14.6.4",
    "nodemon": "^2.0.4",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  }
}
