{
  "name": "ruby-china-graphql-api",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "tsnd -r dotenv/config --respawn src/index.ts",
    "dev:front": "parcel src/playground/index.html",
    "build": "yarn graphql-codegen && yarn build:front && tsc -p tsconfig.json src/index.ts",
    "build:front": "parcel build src/playground/index.html --out-dir src/public",
    "start": "node -r dotenv/config index.js",
    "graphql-codegen": "graphql-codegen --config codegen.yml",
    "graphql-codegen:watch": "graphql-codegen -w --config codegen.yml"
  },
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "apollo-datasource-rest": "^0.9.3",
    "apollo-server-fastify": "^2.16.1",
    "dotenv": "^8.2.0",
    "fastify": "^3.2.0",
    "fastify-oauth2": "^4.1.0",
    "fastify-static": "^3.2.0",
    "graphiql": "^1.0.3",
    "graphql-tools": "^6.0.18",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.20",
    "pino": "^6.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-use": "^15.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/typescript": "^1.17.0",
    "@graphql-codegen/typescript-resolvers": "^1.17.0",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.0.27",
    "@types/pino": "^6.3.0",
    "parcel-bundler": "^1.12.4",
    "ts-node-dev": "^1.0.0-pre.57",
    "typescript": "^3.9.7"
  }
}
